{
  "name": "node-filesystem",
  "version": "0.0.10",
  "description": "A node filesystem manager",
  "main": "index.js",
  "repository": "git@github.com:beautybrands/node-filesystem.git",
  "author": "Piero Giusti <pierophp@gmail.com>",
  "license": "MIT",
  "keywords": [
    "node",
    "file",
    "filesystem",
    "s3",
    "flysystem"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha -A --opts ./mocha.opts --require ts-node/register -r tsconfig-paths/register",
    "test:clear": "clear &&  yarn test",
    "test:watch": "nodemon --exec yarn test:clear",
    "npm:publish": "yarn build && cp package.json dist/ && cd dist/ && npm publish",
    "coverage": "cd coverage && python -m SimpleHTTPServer 8182"
  },
  "dependencies": {
    "aws-sdk": "^2.166.0",
    "fs-extra": "^4.0.3",
    "lodash": "^4.17.4",
    "ltrim": "^1.0.0",
    "mime": "^2.3.1",
    "node-dir": "^0.1.17",
    "rtrim": "^1.0.0",
    "s3": "^4.4.0"
  },
  "devDependencies": {
    "@types/bull": "^3.3.0",
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.4.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.1"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "docker"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "lcovonly",
      "text-summary"
    ],
    "all": true
  }
}
